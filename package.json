{
  "name": "k6chaijs",
  "version": "1.0.0",
  "type": "module",
  "description": "Create a chaijs k6 build",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "engines": {},
  "scripts": {
    "build": "esbuild ./src/index.ts --bundle --log-level=info --external:k6 --format=esm --outfile=build/index.js && npm run build:dts",
    "build:dts": "dts-bundle-generator --no-check --external-inlines @types/chai -o ./build/index.d.ts ./src/index.ts"
  },
  "keywords": [
    "k6"
  ],
  "author": "k6 team",
  "license": "MIT",
  "dependencies": {
    "chai": "^4.5.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/k6": "^0.37.0",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "dts-bundle-generator": "^9.5.1",
    "esbuild": "^0.14.51",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}